{
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "dist",
        "lib": [
          "es2015",
          "es2016",
          "es2017",
          "es2018",
          "es2019",
          "es2020",
          "ESNext",
        ],
        "strict": true,
        "baseUrl": ".",
        "skipLibCheck": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["./cli.ts"],
}

